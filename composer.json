{
  "name": "carno-php/config",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "carno-php/promise": "^1.0",
    "carno-php/channel": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7"
  },
  "suggest": {
    "carno-php/consul": "Loading values from consul-kv"
  },
  "autoload": {
    "psr-4": {
      "Carno\\Config\\": "src"
    },
    "files": [
      "src/utils.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Carno\\Config\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
